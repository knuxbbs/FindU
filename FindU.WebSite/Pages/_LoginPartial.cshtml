@inject UserManager<ApplicationUser> UserManager
@inject SignInManager<ApplicationUser> SignInManager
@if (User.Identity.IsAuthenticated)
{
	<form asp-controller="Account" asp-action="Logout" method="post" id="logoutForm" class="navbar-right">
		<ul class="nav navbar-nav navbar-right">
			<li>
				<a asp-page="/Account/Manage/Index" title="Manage">Hello @UserManager.GetUserName(User)!</a>
			</li>
			<li>
				<button type="submit" class="btn btn-link navbar-btn navbar-link">Log out</button>
			</li>
			<li>
				<a asp-page="/Account/Register">Dados</a>
			</li>
		</ul>
	</form>
}
else
{
	//var externalLogin = (await SignInManager.GetExternalAuthenticationSchemesAsync()).ToList();

	<ul class="nav navbar-nav navbar-right">
		<li><a asp-page="/Account/Login">Entrar com o Facebook</a></li>
		<li><a asp-page="/Account/Register">Dados</a></li>
	</ul>
}
